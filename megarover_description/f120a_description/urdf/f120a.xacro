<?xml version="1.0" ?>
<robot name="f120a" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find f120a_description)/urdf/materials.xacro" />
<!-- <xacro:include filename="$(find f120a_description)/urdf/f120a.trans" /> -->
<xacro:include filename="$(find f120a_description)/urdf/f120a.gazebo" />

<link name="base_footprint" />

<joint name="base_joint" type="fixed">
  <parent link="base_footprint"/>
  <child link="base_link" />
  <origin xyz="-0.108 0 0" rpy="0 0 -1.5708"/>
</joint>

<link name="base_link">
  <inertial>
    <origin xyz="6.540114906371634e-07 -0.003250873034356485 0.120269680948673" rpy="0 0 0"/>
    <mass value="26.535431"/>
    <inertia ixx="0.2129" iyy="0.2979" izz="0.4209" 
             ixy="-0.000036" iyz="0.008147" 
             ixz="0.0000368"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://f120a_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="package://f120a_description/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="right_wheel_1">
  <inertial>
    <origin xyz="0.0 0.0 -0.001" rpy="0 1.5708 0"/>
    <mass value="3.241197"/>
    <inertia ixx="0.01025" iyy="0.00621" izz="0.006211" 
             ixy="0.0" iyz="0.0" 
             ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.085 -0.108 0.202" rpy="0 1.5708 0"/>
    <geometry>
      <mesh filename="package://f120a_description/meshes/right_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="dark_grey"/>
  </visual>
  <collision>
    <origin xyz="-0.085 -0.108 0.202" rpy="0 1.5708 0"/>
    <geometry>
      <mesh filename="package://f120a_description/meshes/right_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="left_wheel_1">
  <inertial>
    <origin xyz="0.0 0.0 0.001" rpy="0 1.5708 0"/>
    <mass value="3.241197"/>
    <inertia ixx="0.01025" iyy="0.00621" izz="0.006211" 
             ixy="0.0" iyz="0.0" 
             ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="-0.085 -0.108 -0.202" rpy="0 1.5708 0"/>
    <geometry>
      <mesh filename="package://f120a_description/meshes/left_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="dark_grey"/>
  </visual>
  <collision>
    <origin xyz="-0.085 -0.108 -0.202" rpy="0 1.5708 0"/>
    <geometry>
      <mesh filename="package://f120a_description/meshes/left_wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="right_wheel_joint" type="continuous">
  <origin xyz="0.202 0.108 0.085" rpy="0 -1.5708 0"/>
  <parent link="base_link"/>
  <child link="right_wheel_1"/>
  <axis xyz="0.0 0.0 1.0"/>
</joint>

<joint name="left_wheel_joint" type="continuous">
  <origin xyz="-0.202 0.108 0.085" rpy="0 -1.5708 0"/>
  <parent link="base_link"/>
  <child link="left_wheel_1"/>
  <axis xyz="0.0 0.0 1.0"/>
</joint>

</robot>
